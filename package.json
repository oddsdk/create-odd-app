{
  "name": "create-webnative-app",
  "version": "0.0.1",
  "keywords": [
    "fission",
    "react",
    "sveltekit",
    "svelte",
    "webnative",
    "wnfs"
  ],
  "description": "Create Webnative apps in either React or SvelteKit with a single command.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fission-codes/create-webnative-app",
    "directory": "./"
  },
  "author": "Fission Team <support@fission.codes>",
  "license": "MIT",
  "bin": {
    "create-webnative-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublishOnly": "cd ../../ && turbo run build",
    "build": "pnpm release"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "async-retry": "1.3.1",
    "chalk": "2.4.2",
    "commander": "9.4.1",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prettier": "2.7.1",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "tar": "4.4.10",
    "typescript": "^4.8.4",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=12.22.0"
  }
}
